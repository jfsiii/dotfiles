# Install native apps
# http://caskroom.io/
brew tap caskroom/cask

# https://github.com/caskroom/homebrew-versions
# The tap for beta/unstable package versions (Chrome Canary, Emacs Nightly, etc)
brew tap caskroom/versions

export HOMEBREW_CASK_OPTS="--appdir=/Applications"
xargs brew cask install < .brew-cask-formulas
